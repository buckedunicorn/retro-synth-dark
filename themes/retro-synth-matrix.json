  "tokenColors": [
    {
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted.single", "string.quoted.double", "string.quoted.triple"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "DocString",
      "scope": ["string.quoted.docstring", "string.quoted.multi", "comment.block.documentation"],
      "settings": {
        "foreground": "#00cccc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.numeric.integer", "constant.numeric.float", "constant.numeric.hex", "constant.numeric.binary", "constant.numeric.octal"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean", "constant.language.true", "constant.language.false"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold"
      }
    },
    {
      "name": "None/null/undefined",
      "scope": ["constant.language.null", "constant.language.undefined", "constant.language.none", "constant.language.nan", "variable.language.this", "variable.language.super"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Python self/cls parameters",
      "scope": ["variable.language.self", "support.variable.self", "variable.parameter.function.language.special.self.python", "variable.parameter.function.language.special.cls.python"],
      "settings": {
        "foreground": "#00cc99",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from", "keyword.control.as", "keyword.import", "keyword.package"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Flow Control",
      "scope": ["keyword.control.flow", "keyword.control.loop", "keyword.control.trycatch", "keyword.control.conditional"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Return/yield",
      "scope": ["keyword.control.return", "keyword.control.yield"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function", "meta.function-call.generic"],
      "settings": {
        "foreground": "#00aaff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Definition",
      "scope": ["storage.type.function", "entity.name.function.definition"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.function-parameter"],
      "settings": {
        "foreground": "#00ff41",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other.readwrite", "variable.other.object", "variable.name", "meta.definition.variable"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "name": "Built-in Variables",
      "scope": ["support.variable.builtin", "variable.language", "support.constant"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class Method",
      "scope": ["meta.method", "meta.method.declaration"],
      "settings": {
        "foreground": "#00aaff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "storage.type.primitive", "storage.type.built-in"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Type Annotation",
      "scope": ["meta.type.annotation", "meta.return.type"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants and Global Variables",
      "scope": ["variable.other.constant", "support.constant"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "keyword.operator.arithmetic", "keyword.operator.assignment", "keyword.operator.comparison", "keyword.operator.logical", "keyword.operator.ternary", "keyword.operator.new"],
      "settings": {
        "foreground": "#ff66ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "meta.object-literal.key", "variable.other.object.property", "support.variable.property"],
      "settings": {
        "foreground": "#33cccc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation Brackets",
      "scope": ["punctuation.definition.parameters", "punctuation.definition.block", "punctuation.definition.arguments", "punctuation.definition.array", "punctuation.section", "meta.brace"],
      "settings": {
        "foreground": "#ff66ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#ff66ff"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "support.class.component", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#33cccc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#ff0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "markup.heading.markdown", "markup.heading.setext"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": {
        "foreground": "#00ff41",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link", "markup.underline.link.markdown", "string.other.link"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown List",
      "scope": ["markup.list", "markup.list.numbered", "markup.list.unnumbered", "punctuation.definition.list"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote", "punctuation.definition.quote"],
      "settings": {
        "foreground": "#00cccc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.fenced_code", "markup.inline.raw", "markup.raw"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "name": "TypeScript Interfaces",
      "scope": ["entity.name.type.interface.ts", "entity.name.type.interface.tsx"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "TypeScript Types",
      "scope": [
        "entity.name.type.ts",
        "entity.name.type.tsx",
        "support.type.primitive.ts",
        "support.type.primitive.tsx",
        "entity.name.type.alias.ts",
        "entity.name.type.alias.tsx"
      ],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "TypeScript Generics",
      "scope": ["meta.type.parameters", "meta.type.annotation"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "name": "TypeScript Decorators",
      "scope": ["meta.decorator", "meta.decorator.ts", "meta.decorator.tsx"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "React/JSX Components",
      "scope": ["support.class.component.jsx", "support.class.component.tsx", "entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python Decorators",
      "scope": ["meta.function.decorator.python", "entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Python Magic Methods",
      "scope": ["support.function.magic.python"],
      "settings": {
        "foreground": "#00aaff",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Python Built-ins",
      "scope": ["support.function.builtin.python", "support.type.python", "support.class.python"],
      "settings": {
        "foreground": "#00aaff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python F-strings",
      "scope": ["meta.fstring.python", "constant.character.format.placeholder.other.python"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "meta.property-name.css"],
      "settings": {
        "foreground": "#33cccc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css", "constant.numeric.css", "meta.property-value.css"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit.css", "keyword.other.unit.scss", "keyword.other.unit.less"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Colors",
      "scope": ["constant.other.color.rgb-value.css", "support.constant.color.w3c-standard-color-name.css"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Pseudo Classes",
      "scope": ["entity.other.attribute-name.pseudo-class.css", "entity.other.attribute-name.pseudo-element.css"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json", "meta.structure.dictionary.key.json"],
      "settings": {
        "foreground": "#33cccc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Values",
      "scope": ["meta.structure.dictionary.value.json"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Template Literals",
      "scope": ["string.template", "punctuation.definition.template-expression", "punctuation.section.embedded"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Embedded Expression",
      "scope": ["meta.embedded", "source.groovy.embedded", "meta.template.expression"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag.html", "punctuation.definition.tag.html"],
      "settings": {
        "foreground": "#ff33cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#33cccc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp", "string.regexp keyword.other", "string.regexp constant.character"],
      "settings": {
        "foreground": "#ff9933",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regex Group",
      "scope": ["punctuation.definition.group.regexp", "meta.group.regexp"],
      "settings": {
